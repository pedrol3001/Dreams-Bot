{
  "name": "oasis-bot",
  "version": "1.0.0",
  "main": "server.ts",
  "private": false,
  "author": "<pedrolcr@outlook.com>",
  "license": "LGPL-3.0",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9229 --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "jest",
    "heroku-postbuild": "npm run build && npm run typeorm -- migration:run -f dist/oasis/config/ormconfig"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.3.1",
    "@types/uuid": "^8.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "discord-dynamic-messages": "^4.0.0",
    "discord-oasis": "^1.0.0",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
