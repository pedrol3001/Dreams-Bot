{
  "name": "oasis-bot",
  "version": "1.0.0",
  "main": "server.ts",
  "private": false,
  "author": "<pedrolcr@outlook.com>",
  "license": "LGPL-3.0",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "db:migrate": "prisma migrate dev",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9229 --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "build": "prisma generate && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/node": "^17.0.0",
    "@types/uuid": "^8.3.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "discord-oasis": "^1.4.0",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
